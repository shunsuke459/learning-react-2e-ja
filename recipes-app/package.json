{
  "name": "recipe-app",
  "version": "1.0.0",
  "description": "Learning React Chapter 5 sample for modular recipes with webpack",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -fR ./dist",
    "build": "npm run clean && mkdir ./dist && cp ./index.html ./dist/ && webpack --mode development",
    "prestart": "npm run build",
    "start": "serve ./dist"
  },
  "keywords": [
    "Learning",
    "React",
    "webpack",
    "intro",
    "jsx"
  ],
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve": "^11.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
